{
  "name": "ptuchik/core-utilities",
  "description": "Core utilities for Laravel to extend some functionality and prepare project for other related packages",
  "keywords": [
    "ptuchik",
    "core",
    "laravel",
    "utilites",
    "translatable",
    "storage",
    "google"
  ],
  "license": "MIT",
  "type": "library",
  "require": {
    "illuminate/support": "^7.0|^8.0",
    "spatie/laravel-translatable": "^4.0",
    "superbalist/laravel-google-cloud-storage": "^2.0",
    "intervention/image": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Ptuchik\\CoreUtilities\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Avik Aghajanyan",
      "email": "avik.aghajanyan@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Ptuchik\\CoreUtilities\\Providers\\CoreUtilitiesServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
